{
  "name": "primebeyonder/laravel-upload-service",
  "description": "Laravel Upload Service Package (Local, DigitalOcean, AWS ready)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name",
      "email": "your@email.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "PrimeBeyonder\\LaravelUploadService\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PrimeBeyonder\\LaravelUploadService\\Providers\\UploadServiceProvider"
      ],
      "aliases": {
        "Upload": "PrimeBeyonder\\LaravelUploadService\\Facades\\UploadFacade"
      }
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

{
  "name": "primebeyonder/laravel-upload-service",
  "description": "A lightweight Laravel package for abstracted file uploads.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew",
      "email": "thum14252@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10|^11",
    "illuminate/filesystem": "^10|^11",
    "league/flysystem-aws-s3-v3": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Primebeyonder\\LaravelUploadService\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Primebeyonder\\LaravelUploadService\\Providers\\UploadServiceProvider"
      ],
      "aliases": {
        "Upload": "Primebeyonder\\LaravelUploadService\\Facades\\UploadFacade"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
